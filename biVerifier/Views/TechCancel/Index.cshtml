@model List<biVerifier.Models.TechCancelData>

@{
    ViewData["Title"] = "TECH CANCEL";

}


<div class="custom-container" style="margin-top: 4rem">
    <div class="row">
        <div class="col-md-12">
            <div class="box">
                <div class="box-body">
                    <div class="">
                        <div class="d-flex  align-items-center justify-content-between mb-20">
                            <div>
                                <h5 style="text-transform: uppercase ">LIST TECH CANCELS</h5>
                            </div>
                            <div>
                                <button class="btn btn-primary">EXPORT TO EXCEL</button>
                            </div>
                        </div>
                    </div>
                    <div class="table-responsive rounded card-table">
                        <table class="table table-striped table-hover">
                            <thead>
                                <tr>
                                    <th>ID</th>
                                    <th>Client Name</th>
                                    <th>Site ID </th>
                                    <th>Date </th>
                                    <th>Tech Responsibility</th>
                                    <th>Total Channels</th>
                                    <th>Street</th>
                                    <th>Suburb</th>
                                    <th>Reason</th>
                                    <th>Notes</th>
                                    <th>Reduced_Value_Ex_Vat</th>
                                    <th>TechResponsible</th>
                                    <th>Total_Channels</th>

                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var cData in Model)
                                {
                                    <tr class="hover-primary">

                                        <td>@cData?.ID</td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td></td>

                                    </tr>

                                }

                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>